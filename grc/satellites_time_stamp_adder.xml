<?xml version="1.0"?>
<block>
  <name>time_stamp_adder</name>
  <key>satellites_time_stamp_adder</key>
  <category>[Satellites]/CCSDS</category>
  <import>import satellites</import>
  <make>satellites.time_stamp_adder($input_manual_automatic)</make>
  <param>
    <name>Manual or Automatic Time</name>
    <key>manual_automatic</key>
    <value>drop_down</value>
    <type>enum</type>
    <option>
	<name>Manual</name>
	<key>0</key>
    </option>
    <option>
	<name>Automatic</name>
	<key>1</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
