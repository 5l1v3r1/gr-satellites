<?xml version="1.0"?>
<block>
  <name>telemetry_parser</name>
  <key>satellites_telemetry_parser</key>
  <category>[Satellites]/CCSDS</category>
  <import>import satellites</import>
  <make>satellites.telemetry_parser($coding, $options)</make>
<param>
    <name>Coding</name>
    <key>coding</key>
    <value>drop_down</value>
    <type>enum</type>
    <hide>none</hide>
    <option>
	<name>Uncoded</name>
	<key>1</key>
    </option>
    <option>
	<name>Convolutional</name>
	<key>2</key>
    </option>
    <option>
	<name>Reed Solomon</name>
	<key>3</key>
    </option>
    <option>
	<name>Concatenated</name>
	<key>4</key>
    </option>
    <option>
	<name>Turbo</name>
	<key>5</key>
    </option>
    <option>
	<name>LDPC on a Transfer Frame</name>
	<key>6</key>
    </option>
    <option>
	<name>LDPC on a Stream of SMTFS</name>
	<key>7</key>
    </option>
  </param>
  <param>
    <name>Options</name>
    <key>options</key>
    <value>drop_down</value>
    <type>enum</type>
    <hide>none</hide>
    <option>
	<name>Uncoded</name>
	<key>1</key>
    </option>
    <option>
	<name>Convolutional</name>
	<key>2</key>
    </option>
    <option>
	<name>Reed Solomon</name>
	<key>3</key>
    </option>
    <option>
	<name>Concatenated</name>
	<key>4</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
